{
    "version": 1,
    "disable_existing_loggers": true,
    "formatters": {
        "console": {
            "format": "%(asctime)s [%(levelname)s] - %(message)s"
        }
    },
    "handlers": {
        "console": {
            "level": "INFO",
            "formatter": "console",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "": {
            "handlers": ["console"],
            "level": "INFO",
            "propagate": false
        }
    }
}
